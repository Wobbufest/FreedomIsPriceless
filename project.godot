quality/shadow_atlas/quadrant_0_subdiv=3
quality/shadow_atlas/quadrant_1_subdiv=3
quality/shadow_atlas/quadrant_3_subdiv=3
quality/shadows/filter_mode=2
environment/default_clear_color=Color( 0, 0, 0, 1 )
environment/default_environment="res://Editor Environment.tres"
